{
  "name": "react-docker-environment",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "env-cmd": "^8.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.0"
  },
  "scripts": {
    "start": "env-cmd .env.dev react-scripts start",
    "build": "env-cmd .env.dev react-scripts build",
    "build:dev": "env-cmd .env.dev react-scripts build",
    "build:qa": "env-cmd .env.qa react-scripts build",
    "build:prod": "env-cmd .env.prod react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
